<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Simple Smoke Test Suite Parallel Execution" verbose="1" parallel="none" >

	<listeners>
		<listener class-name="com.sample.report.custReportListener"/>
		<listener class-name="com.sample.report.testListener"/>
		<listener class-name="com.sample.report.reportListener"/>
		<listener class-name="org.testng.reporters.EmailableReporter"/>
	</listeners>

	<test name="Simple Smoke Test" >
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.DemoQATest.QADemoElementTest" />
		</classes>
	</test>
</suite>

